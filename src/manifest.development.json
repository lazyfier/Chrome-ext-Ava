{
    "manifest_version": 2,
    "name": "ava",
    "description": "ava",
    "version": "1",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions":
	[
        "downloads",
        "activeTab",
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
        "<all_urls>"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}